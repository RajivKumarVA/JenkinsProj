
pipeline {
  agent {
    label 'deploy'
  }
  stages {
    stage ('Test Stage') {
      steps {
        echo "Test Mode"
        git 'https://github.com/RajivKumarVA/JenkinsProj.git'
      }
    }
    
    stage ('Deploy Stage') {
      steps {
        echo "Deploy Stage"
        sh deployWeb.sh
      }
    }
  }
}
     
